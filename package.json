{
  "name": "react-app",
  "description": "React web application scaffold",
  "version": "2.0.0",
  "author": "kaysonwu <kayson@huida.cn>",
  "license": "MIT",
  "scripts": {
    "start": "node ./server/server.js",
    "dev": "webpack-dev-server --env=dev --target=web --config=./config/webpack.js",
    "dev:ssr": "yarn dev --target=node && yarn build:server && yarn start",
    "build": "webpack --env=prod --target=web --config=./config/webpack.js",
    "build:ssr": "yarn build --target=node && yarn build:server",
    "build:server": "babel -o server/server.js --config-file ./config/babel.node.json ./src/server.tsx",
    "build:models": "babel -d server/models --config-file ./config/babel.node.json src/models --extensions .ts",
    "build:node": "webpack --env=prod --target=node --config=./config/webpack.js",
    "test": "jest"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "antd": "^4.0.3",
    "axios": "^0.19.0",
    "classnames": "^2.2.0",
    "cookie": "^0.3.0",
    "core-js": "^3.6.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-intl": "^4.0.0",
    "react-redux": "^7.0.0",
    "react-router-dom": "^5.1.0",
    "redux": "^4.0.0",
    "redux-saga": "^1.0.0",
    "regenerator-runtime": "^0.13.0",
    "serve-static": "^1.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "@types/classnames": "^2.2.0",
    "@types/cookie": "^0.3.0",
    "@types/jest": "^25.1.0",
    "@types/loadable__component": "^5.10.0",
    "@types/loadable__server": "^5.12.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "@types/react-redux": "^7.0.0",
    "@types/react-router-dom": "^5.1.0",
    "@types/serve-static": "^1.13.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-import": "^1.9.0",
    "clean-webpack-plugin": "^3.0.0",
    "commander": "^4.0.0",
    "css-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "less": "^3.0.0",
    "less-loader": "^5.0.0",
    "style-loader": "^1.0.0",
    "typescript": "^3.5.1",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.0.0",
    "webpack-dev-server": "^3.1.0",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.7.2",
    "webpack-preprocessor-loader": "^1.1.2"
  }
}
